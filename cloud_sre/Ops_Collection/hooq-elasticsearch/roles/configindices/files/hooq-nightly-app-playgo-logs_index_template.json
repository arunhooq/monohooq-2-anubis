{
  "order": 10,
  "index_patterns": "hooq-nightly-app-playgo-*",
  "settings": {
    "index": {
      "number_of_shards": "2",
      "mapping.ignore_malformed": true
    }
  },
  "mappings": {
      "dynamic_templates": [
        {
          "integers": {
            "match_mapping_type": "long",
            "mapping": {
              "type": "integer"
            }
          }
        },
        {
          "strings": {
            "match_mapping_type": "string",
            "mapping": {
              "type": "text",
              "fields": {
                "raw": {
                  "type": "keyword",
                  "ignore_above": 256
                }
              }
            }
          }
        },
        {
          "dates_as_keyword": {
            "mapping": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "match_mapping_type": "date"
          }
        }
      ],
      "properties": {
        "time": {
          "format": "yyyy-MM-dd'T'HH:mm:ss.SSS Z",
          "type": "date"
        },
        "message": {
          "type": "text",
          "analyzer": "standard"
        },
        "host": {
          "type": "text",
          "analyzer": "standard"
        },
        "user_agent": {
          "type": "text",
          "analyzer": "standard"
        },
        "error": {
          "type": "text",
          "analyzer": "standard"
        },
        "latency_human": {
          "type": "text",
          "analyzer": "standard"
        },
        "@timestamp": {
          "type": "date"
        }
      }
    }
  }